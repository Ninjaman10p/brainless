if and(True, True):
    print("True and True")
if and(True, False):
    print("True and False")
if and(False, True):
    print("False and True")
if and(False, False):
    print("False and False")
    
if or(True, True):
    print("True or True")
if or(True, False):
    print("True or False")
if or(False, True):
    print("False or True")
if or(False, False):
    print("False or False")
    
